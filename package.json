{
    "name": "filter-line",
    "displayName": "Filter Line",
    "description": "filter line by string or regular expression",
    "version": "0.0.1",
    "publisher": "everettjf",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.filterLineWithInputString",
        "onCommand:extension.filterLineWithInputRegex",
        "onCommand:extension.filterLineWithConfigFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.filterLineWithInputString",
                "title": "Filter Line With Input String"
            },
            {
                "command": "extension.filterLineWithInputRegex",
                "title": "Filter Line With Input Regex"
            },
            {
                "command": "extension.filterLineWithConfigFile",
                "title": "Filter Line With Config File"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.9.2",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
